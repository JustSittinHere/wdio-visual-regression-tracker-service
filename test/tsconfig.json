{
    "compilerOptions": {
        "outDir": "dist",
        "module": "commonjs",
        "target": "es6",
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "noImplicitAny": false,
        "types": ["node", "webdriverio/async", "@wdio/types", "wdio-visual-regression-tracker-service", "@wdio/jasmine-framework"],
        "skipLibCheck": true,
        "moduleResolution": "node",
    },
    "include": ["specs"]
}
