{
    "compilerOptions": {
        "outDir": "dist",
        "module": "esnext",
        "target": "esnext",
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "noImplicitAny": false,
        "types": ["node", "@wdio/globals/types", "wdio-visual-regression-tracker-service", "@wdio/jasmine-framework"],
        "skipLibCheck": true,
        "allowSyntheticDefaultImports": true
    },
    "include": ["**/*.ts"]
}
